{
  "name": "climood",
  "description": "Mood tracking cli app",
  "version": "0.0.2",
  "author": "tn @tn",
  "bin": {
    "climood": "./bin/run"
  },
  "bugs": "https://github.com/tn/climood/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "asciichart": "^1.5.25",
    "boxen": "^5.0.1",
    "chalk": "^4.1.1",
    "date-fns": "^2.23.0",
    "enquirer": "^2.3.6",
    "inquirer": "^8.0.0",
    "listr2": "^3.9.0",
    "node-emoji": "^1.10.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/asciichart": "^1.5.5",
    "@types/chai": "^4",
    "@types/chalk": "^2.2.0",
    "@types/inquirer": "^7.3.1",
    "@types/listr": "^0.14.3",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/node-emoji": "^1.8.1",
    "@types/sinon": "^10.0.2",
    "chai": "^4",
    "eslint": "^5.13",
    "eslint-config-oclif": "^3.1",
    "eslint-config-oclif-typescript": "^0.1",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "rimraf": "^3.0.2",
    "sinon": "^11.1.2",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/tn/climood",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "climood",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "tn/climood",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
